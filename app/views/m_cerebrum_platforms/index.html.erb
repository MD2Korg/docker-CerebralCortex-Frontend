<p id="notice"><%= notice %></p>

<h1>Listing M Cerebrum Platforms</h1>

<table>
  <thead>
    <tr>
      <th>Platformtype</th>
      <th>Identifier</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @m_cerebrum_platforms.each do |m_cerebrum_platform| %>
      <tr>
        <td><%= m_cerebrum_platform.platformtype %></td>
        <td><%= m_cerebrum_platform.identifier %></td>
        <td><%= link_to 'Show', m_cerebrum_platform %></td>
        <td><%= link_to 'Edit', edit_m_cerebrum_platform_path(m_cerebrum_platform) %></td>
        <td><%= link_to 'Destroy', m_cerebrum_platform, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New M cerebrum platform', new_m_cerebrum_platform_path %>
